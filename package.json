{
  "name": "open-pos-frontend",
  "version": "0.1.0",
  "private": false,
  "description": "Open Source Point of Sale system",
  "keywords": [
    "angular",
    "components",
    "reusable"
  ],
  "scripts": {
    "e2e": "protractor",
    "e2e-test": "protractor ./protractor.conf.js",
    "tslint": "tslint -c ./tslint.json \"./src/**/*.ts\" -e \"./src/**/typings.d.ts\" -e \"./src/environments/**\"",
    "npm-install": "npm --prefix ./dist install ./dist --production",
    "postinstall": "webdriver-manager update",
    "lint": "tslint --format codeFrame -c ./tslint.json \"./src/**/*.ts\" -e \"./src/**/typings.d.ts\" -e \"./src/environments/**\"",
    "start": "ng serve --live-reload --watcher",
    "aot": "./node_modules/.bin/ngc -p deploy/platform/tsconfig-aot.json",
    "start-api": "cd mock-api && covalent-data &",
    "stop-api": "pkill covalent-data",
    "webdriver-update": "bash ./node_modules/.bin/webdriver-manager update",
    "test": "ng test --code-coverage --single-run",
    "ghpages-deploy": "ng build --base-href /covalent-quickstart/ prod && bash scripts/ghpages-deploy.sh",
    "live-reload": "gulp watch"
  },
  "engines": {
    "node": ">4.4 < 7",
    "npm": ">3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/saurabh1e/open-pos-frontend"
  },
  "license": "GPLA3",
  "author": "Saurabh Gupta",
  "contributors": [
    "Saurabh Gupta <saurabh.1e1@gmail.com>"
  ],
  "dependencies": {
    "@angular/common": "2.4.5",
    "@angular/compiler": "2.4.5",
    "@angular/core": "2.4.5",
    "@angular/forms": "2.4.5",
    "@angular/http": "2.4.5",
    "@angular/material": "2.0.0-beta.1",
    "@angular/platform-browser": "2.4.5",
    "@angular/platform-browser-dynamic": "2.4.5",
    "@angular/platform-server": "2.4.5",
    "@angular/router": "3.4.5",
    "@covalent/charts": "1.0.0-beta.1",
    "@covalent/core": "1.0.0-beta.1",
    "@covalent/dynamic-forms": "1.0.0-beta.1",
    "@covalent/highlight": "1.0.0-beta.1",
    "@covalent/http": "1.0.0-beta.1",
    "@covalent/markdown": "1.0.0-beta.1",
    "@swimlane/ngx-charts": "^3.1.2",
    "core-js": "^2.4.1",
    "d3": "^4.4.0",
    "dexie": "^2.0.0-beta.10",
    "hammerjs": "^2.0.8",
    "highlight.js": "9.9.0",
    "moment": "^2.17.1",
    "ng2-tag-input": "^0.8.5",
    "rxjs": "^5.1.0",
    "showdown": "1.6.0",
    "zone.js": "^0.7.2"
  },
  "devDependencies": {
    "@angular/compiler-cli": "2.4.5",
    "@types/hammerjs": "2.0.30",
    "@types/jasmine": "2.2.31",
    "@types/node": "6.0.34",
    "@types/selenium-webdriver": "2.53.36",
    "angular-cli": "1.0.0-beta.26",
    "codelyzer": "2.0.0-beta.4",
    "electron": "^1.5.0",
    "electron-connect": "^0.6.1",
    "electron-packager": "^8.5.1",
    "ember-cli-inject-live-reload": "1.4.0",
    "gulp": "3.9.1",
    "gulp-help": "1.6.1",
    "immutable": "^3.8.1",
    "jasmine-core": "2.4.1",
    "jasmine-spec-reporter": "2.5.0",
    "karma": "1.2.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-cli": "1.0.1",
    "karma-jasmine": "1.0.2",
    "karma-remap-istanbul": "0.2.1",
    "ng2-cookies": "^1.0.4",
    "ng2-pagination": "^2.0.0",
    "node-sass": "3.8.0",
    "phantomjs-prebuilt": "2.1.7",
